<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3F51B5"
    android:orientation="vertical"
    tools:context=".FallEventList">

    <TextView
        android:id="@+id/falllist1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="My Fall Event Logs:"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/falllist2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Quick Tip: You can tap on each timestamp to see further details about each fall event log."
        android:textColor="#303030"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="match_parent"
        android:layout_height="49dp" />

    <ListView
        android:id="@+id/listviewFall"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>